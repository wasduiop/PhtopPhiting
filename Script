local DrRayLibrary = loadstring(game:HttpGet("https://raw.githubusercontent.com/AZYsGithub/DrRay-UI-Library/main/DrRay.lua"))()

local window = DrRayLibrary:Load("DrRay", "Default")

local tab = DrRayLibrary.newTab("My Tab", "ImageIdHere")

tab.newButton("nobounce", "Prints Hello!", function()
    print("Bouncer removed")
    game.Workspace.Monsters.Bouncer:Destroy()
end)

tab.newButton("obstruct", "Prints Hello!", function()
    print('nodance')

    game.Workspace.Rooms:GetChildren():GetChildren(Interactables).SkeletonDancer:Destroy()
        
end)
